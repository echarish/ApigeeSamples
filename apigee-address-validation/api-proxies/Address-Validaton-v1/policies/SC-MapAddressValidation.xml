<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ServiceCallout continueOnError="false" enabled="true" name="SC-MapAddressValidation">
    <DisplayName>SC-MapAddressValidation</DisplayName>
    <Properties/>
    <Request variable="requestPayload">
        <Set>
            <Verb>POST</Verb>
            <Headers>
                <Header name="Content-Type">application/json</Header>
            </Headers>
            <QueryParams>
                <QueryParam name="key">{gmaps_key}</QueryParam>
            </QueryParams>
        </Set>
        <IgnoreUnresolvedVariables>false</IgnoreUnresolvedVariables>
    </Request>
    <Response>response</Response>
    <HTTPTargetConnection>
        <Properties/>
        <URL>https://addressvalidation.googleapis.com/v1:validateAddress</URL>
    </HTTPTargetConnection>
